class Arithmetic
{
public static void main(String[] args)
{
int a=10;
int b=5;
int c=15;
int d=20;
System.out.println("Addition result:"+(a+b));
System.out.println("Subtraction result:"+(d-c));
System.out.println("Multiplication result:"+(b*c));
System.out.println("Division result:"+(d/a));
System.out.println("remainder:"+(b%c));
}
}